<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.buzzard.db.models.FileNameTable" table="filenametable">
		
		<id column="id" name="id" type="long">
			<generator class="increment"/>
		</id>
		
		<property column="user_id" name="userID" type="long"/>
		<property column="file_id" name="fileID" type="long"/>
		<property column="file_name" name="fileName" type="java.lang.String"/>
		
	</class>
</hibernate-mapping>
